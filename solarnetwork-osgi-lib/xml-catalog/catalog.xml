<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">

	<rewriteSystem 
		systemIdStartString="http://www.oasis-open.org/committees/entity/release/1.0/"
		rewritePrefix="catalog/"/>

	<system systemId="-//OASIS/DTD Entity Resolution XML Catalog V1.0//EN" 
		uri="catalog/catalog.dtd"/>

	<!-- Ibatis SqlMap support -->

	<rewriteSystem 
		systemIdStartString="http://ibatis.apache.org/dtd/"
		rewritePrefix="catalog/"/>

	<system systemId="-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
		uri="catalog/sql-map-2.dtd"/>

	<system systemId="-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
		uri="catalog/sql-map-config-2.dtd"/>
		
	<!-- MyBatis support -->

	<rewriteSystem 
		systemIdStartString="http://mybatis.org/dtd/"
		rewritePrefix="catalog/"/>

	<system systemId="-//mybatis.org//DTD Mapper 3.0//EN" 
		uri="catalog/mybatis-3-mapper.dtd"/>

	<!-- Tiles support -->
	
	<rewriteSystem 
		systemIdStartString="http://tiles.apache.org/dtds/"
		rewritePrefix="catalog/"/>

	<system systemId="-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN" 
		uri="catalog/tiles-config_2_0.dtd"/>
	
	<system systemId="-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
		uri="catalog/tiles-config_3_0.dtd"/>
	
	<nextCatalog catalog="catalog/jee/catalog.xml"/>
	<nextCatalog catalog="catalog/spring/catalog.xml"/>
	<nextCatalog catalog="catalog/gemini/catalog.xml"/>
	<nextCatalog catalog="catalog/cxf/catalog.xml"/>
	
</catalog>
