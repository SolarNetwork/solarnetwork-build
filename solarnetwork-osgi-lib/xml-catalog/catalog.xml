<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE catalog
   PUBLIC "-//OASIS/DTD Entity Resolution XML Catalog V1.0//EN"
   "http://www.oasis-open.org/committees/entity/release/1.0/catalog.dtd">
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">

	<rewriteSystem 
		systemIdStartString="http://www.oasis-open.org/committees/entity/release/1.0/"
		rewritePrefix="catalog/"/>

	<public publicId="-//OASIS/DTD Entity Resolution XML Catalog V1.0//EN" 
		uri="catalog/catalog.dtd"/>

	<!-- Ibatis SqlMap support -->

	<rewriteSystem 
		systemIdStartString="http://ibatis.apache.org/dtd/"
		rewritePrefix="catalog/"/>

	<public publicId="-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
		uri="catalog/sql-map-2.dtd"/>

	<public publicId="-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" 
		uri="catalog/sql-map-config-2.dtd"/>
		
	<!-- Tiles support -->
	
	<rewriteSystem 
		systemIdStartString="http://tiles.apache.org/dtds/"
		rewritePrefix="catalog/"/>

	<public publicId="-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
		uri="catalog/tiles-config_2_0.dtd"/>
	
	<nextCatalog catalog="catalog/jee/catalog.xml"/>
	<nextCatalog catalog="catalog/spring/catalog.xml"/>
	
</catalog>
