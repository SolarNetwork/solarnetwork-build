<?xml version="1.0" encoding="UTF-8"?>
<!--
	This is an example SolarNode deployment configuration for Protobuf support.
-->
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNode-Protobuf"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->default(runtime)">

		<!-- Protobuf Support -->
		<dependency org="com.google.protobuf" name="protobuf-java" rev="3.7.1" />

		<!-- Global excludes provided by the base system -->

	</dependencies>
</ivy-module>
