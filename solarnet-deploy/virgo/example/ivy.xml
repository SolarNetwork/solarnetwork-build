<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
	<info organisation="SolarNetwork" module="SolarNet"/>
	<configurations>
		<conf name="runtime" visibility="public" description="The Runtime"/>
	</configurations>
	<dependencies defaultconfmapping="runtime->runtime(default)">
		<!-- Apache Commons -->
		<dependency org="commons-beanutils" name="commons-beanutils" rev="1.8.3">
			<artifact name="commons-beanutils" type="jar" />
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="commons-collections" name="commons-collections" rev="3.2.1">
			<artifact name="commons-collections" type="jar" />
			<exclude name="commons-logging"/>
		</dependency>
    	<dependency org="commons-codec" name="commons-codec" rev="1.7" >
    		<artifact name="commons-codec" type="jar"/>
			<exclude name="commons-logging"/>
    	</dependency>
		<dependency org="commons-io" name="commons-io" rev="2.4" >
    		<artifact name="commons-io" type="jar" />
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="commons-fileupload" name="commons-fileupload" rev="1.2.2" >
    		<artifact name="commons-fileupload" type="jar" />
			<exclude name="commons-logging"/>
		</dependency>
		
		<!-- CSV -->
    	<dependency org="org.apache.commons" name="com.springsource.org.apache.commons.csv" rev="1.0.0.BUILD-20080106" />
		<dependency org="net.sf.supercsv" name="super-csv" rev="2.0.1" >
			<artifact name="super-csv" type="jar" />
		</dependency>
		
		<!-- JSON -->
		<dependency org="org.codehaus.jackson" name="jackson-core-asl" rev="1.9.5">
			<artifact name="jackson-core-asl" type="jar" />
		</dependency>
		<dependency org="org.codehaus.jackson" name="jackson-mapper-asl" rev="1.9.5">
    		<artifact name="jackson-mapper-asl" type="jar" />
		</dependency>	
		
		<!-- JDBC -->
		<dependency org="org.apache.tomcat" name="tomcat-jdbc" rev="7.0.29">
    		<artifact name="tomcat-jdbc" type="jar" />
    		<exclude module="tomcat-juli"/><!-- Ignore Tomcat version, which isn't OSGi -->
		</dependency>
		<dependency org="org.apache.juli.springsource" name="com.springsource.org.apache.juli.extras.springsource" rev="6.0.32.S2-r1673" />
		<dependency org="org.apache.ibatis" name="com.springsource.com.ibatis" rev="2.3.4.726" />
		
		<!-- Spring -->
		<dependency org="org.springframework" name="spring-aop" rev="3.0.7.RELEASE">
    		<artifact name="spring-aop" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-asm" rev="3.0.7.RELEASE">
    		<artifact name="spring-asm" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-beans" rev="3.0.7.RELEASE">
    		<artifact name="spring-beans" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-context" rev="3.0.7.RELEASE">
    		<artifact name="spring-context" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-context-support" rev="3.0.7.RELEASE">
    		<artifact name="spring-context-support" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-expression" rev="3.0.7.RELEASE">
    		<artifact name="spring-expression" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-core" rev="3.0.7.RELEASE">
    		<artifact name="spring-core" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-jdbc" rev="3.0.7.RELEASE">
    		<artifact name="spring-jdbc" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-orm" rev="3.0.7.RELEASE">
    		<artifact name="spring-orm" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-tx" rev="3.0.7.RELEASE">
    		<artifact name="spring-tx" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-web" rev="3.0.7.RELEASE">
    		<artifact name="spring-web" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework" name="spring-webmvc" rev="3.0.7.RELEASE">
    		<artifact name="spring-webmvc" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		
		<!--  Spring WebFlow -->
		<dependency org="org.springframework.webflow" name="spring-webflow" rev="2.3.0.RELEASE">
			<artifact name="spring-webflow" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		
		<!-- Spring Security -->
		<dependency org="org.springframework.security" name="spring-security-config" rev="3.1.3.RELEASE">
    		<artifact name="spring-security-config" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework.security" name="spring-security-core" rev="3.1.3.RELEASE">
    		<artifact name="spring-security-core" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework.security" name="spring-security-web" rev="3.1.3.RELEASE">
    		<artifact name="spring-security-web" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>
		<dependency org="org.springframework.security" name="spring-security-taglibs" rev="3.1.3.RELEASE">
    		<artifact name="spring-security-taglibs" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="commons-logging"/>
		</dependency>

		<!--  OSGi -->
		<dependency org="org.osgi" name="org.osgi.enterprise" rev="4.2.0" >
    		<artifact name="org.osgi.enterprise" type="jar" />
		</dependency>
		
		<!-- Apache Tiles -->
		<dependency org="org.apache.tiles" name="tiles-el" rev="2.2.2" >
    		<artifact name="tiles-el" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="slf4j-api"/>
			<exclude name="jcl-over-slf4j"/>
		</dependency>
		<dependency org="org.apache.tiles" name="tiles-jsp" rev="2.2.2" >
    		<artifact name="tiles-jsp" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="slf4j-api"/>
			<exclude name="jcl-over-slf4j"/>
		</dependency>
		<dependency org="org.apache.tiles" name="tiles-servlet" rev="2.2.2" >
    		<artifact name="tiles-servlet" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="slf4j-api"/>
			<exclude name="jcl-over-slf4j"/>
		</dependency>
		<dependency org="org.apache.tiles" name="tiles-servlet-wildcard" rev="2.2.2" >
			<artifact name="tiles-servlet-wildcard" type="jar" />
    		<exclude name="aopalliance"/>
			<exclude name="slf4j-api"/>
			<exclude name="jcl-over-slf4j"/>
		</dependency>
		
		<!-- JodaTime -->
		<dependency org="joda-time" name="joda-time" rev="2.1" >
    		<artifact name="joda-time" type="jar" />
		</dependency>
		<dependency org="joda-time" name="joda-time-jsptags" rev="1.1.1">
    		<artifact name="joda-time-jsptags" type="jar" />
		</dependency>
		
		<!-- URL Rewrite -->
		<dependency org="org.tuckey" name="urlrewritefilter" rev="4.0.4" conf="runtime">
			<!--exclude name="com.springsource.org.apache.commons.logging"/>
			<exclude name="com.springsource.org.apache.log4j"/-->
		</dependency>
			
	</dependencies>
</ivy-module>
